<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>numm__TiersObligatoire</fullName>
    <active>true</active>
    <description>Le tiers est obligatoire pour les journaux de Banque.</description>
    <errorConditionFormula>AND(
TEXT( numm__CategorieJournal__c ) = &#39;Banque&#39;,
 ISBLANK( numm__IdThirdParty__c )
)</errorConditionFormula>
    <errorDisplayField>numm__IdThirdParty__c</errorDisplayField>
    <errorMessage>Le tiers est obligatoire pour les journaux de Banque.</errorMessage>
</ValidationRule>
